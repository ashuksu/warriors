{
    "name": "ashuksu/warriors",
    "description": "Warriors Project: a modern PHP application leveraging Vite and Docker.",
    "type": "project",
    "license": "ISC",
    "authors": [
        {
            "name": "ashuksu",
            "email": "ashuksu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/config.php",
            "app/Helpers/helpers.php",
            "app/Helpers/Vite.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "vlucas/phpdotenv": "^5.6",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "ext-apcu": "*"
    },
    "require-dev": {
        "robmorgan/phinx": "^0.13.4",
        "symfony/var-dumper": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}