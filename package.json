{
  "scripts": {
    "serve": "live-server",
    "start": "php -S 0.0.0.0:$PORT",
    "build": "echo 'Build completed successfully'",
    "docker:build": "docker build -t warriors .",
    "docker:start": "docker run -p 8080:80 warriors",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=warriors)",
    "docker:dev": "docker run -p 8080:80 -v ${PWD}:/var/www/html warriors"
  }
}